 <div> 
<h1 id="logosTitle">Logos</h1>
    <div class="row" id="profile-grid">
        
	<% sql.forEach(function (logos) { %>

            <div class="col-sm-4 col-xs-12 profile">

		<div class="panel panel-default">

                    <div class="panel-thumbnail">
			<a href="#" title="<%= logos.logoName %>" class="thumb">
                            <img src="assets/user_icons/<%= logos.customerId+logos.logoID+logos.logoName %>" class="img-responsive img-rounded" data-toggle="modal" data-target=".modal-profile-lg">
			</a>
                    </div>

                    <div class="panel-body">

			<p class="profile-name"><%= logos.logoName %></p>

                        <form action="#" method="post">

                            <input type="hidden" name='pictureId' value="<%= logos.logoID %>">

                            <button type="submit" formaction="/getOneLogo">download</button>

                            <button type-"submit" formaction="/removeOneLogo">delete</button>

                   	</form>  
                    </div>
		</div>
            </div>
                                   
      	<% }) %>
    </div>
<!-- .modal-profile -->
	<div class="modal fade modal-profile" tabindex="-1" role="dialog" aria-labelledby="modalProfile" aria-hidden="true">
		<div class="modal-dialog modal-lg">
			<div class="modal-content">
				<div class="modal-header">
                                    <button class="close" type="button" data-dismiss="modal">Ã—</button>
                                    <h3 class="modal-title"></h3>
				</div>
				<div class="modal-body">
				</div>
				<div class="modal-footer">
					<button class="btn btn-default" data-dismiss="modal">Close</button>
				</div>
			</div>
        
		</div>
	</div>
<!-- //.modal-profile -->

</div>
